    <script>
        $(document).ready(function() {
             if ($("#ReferenceID").val() == '0') {
                $('#exampleModal').modal('show');   
                setInterval(function() {$('#exampleModal').modal('show');}, 10000);            
            }
            
        });
        $(".close").on('click', function() {
            $("#exampleModal").modal('hide');
        });
        var otpTxnId = null;

        function onGenerateOTPRe() {

            var aadhaarNumber = $('#AadhaarNumber').val();
            if (aadhaarNumber.trim().length !== 12) {
                alert("Please enter valid aadhaar number")
                $('#AadhaarNumber').focus();
                return;
            }
            $.ajax({
                type: 'POST',
                url: "/frontend/web/aadhaarreverify/generateotp",
                data: {
                    AADHAARNUMBER: aadhaarNumber
                },
                "beforeSend": function(response) {
                    SimpleLoading.start();
                },
                success: function(data) {
                    var resp = JSON.parse(data);
                    SimpleLoading.stop();
                    if (resp.error) {
                        $('#spnResult').html(resp.errorMessage);
                        $('#spnResult').css("color", "red");
                        $('#divMessage').show();
                        $("#otpdiv").hide();
                        $(".AadhaarNumberhide").show();
                        return false;
                    }

                    otpTxnId = resp.otpTxnId;
                    // console.log(otpTxnId);
                    var message = "OTP send successfully";
                    var mobileAdded = false;
                    if (resp.maskedMobile !== undefined && resp.maskedMobile !== "" && resp.maskedMobile !== "NA" && resp.maskedMobile !== null) {
                        message = message + " to " + resp.maskedMobile;
                        mobileAdded = true;
                    }

                    if (resp.maskedEmail !== undefined && resp.maskedEmail !== "" && resp.maskedEmail !== null) {
                        if (mobileAdded) {
                            message = message + " and";
                        } else {
                            message = message + " to";
                        }

                        message = message + " " + resp.maskedEmail;
                    }


                    $('#spnResult').html(message);
                    $('#spnResult').css("color", "green");
                    $('#divMessage').show();
                    $("#otpdiv").show();
                    $(".AadhaarNumberhide").hide();
                    $('#btnGenerateOTP').attr("disabled", true);
                    $('#btnValidateOTP').removeAttr("disabled");
                    $('#btnCancel').removeAttr("disabled");
                    $('#Otp').removeAttr("disabled");
                },
                error: function(xhr, textStatus, error) {
                    $('#spnResult').html(error.message);
                    $('#spnResult').css("color", "red");
                    $('#divMessage').show();
                    return false;
                }
            });
        }

        function onValidateOtpRe() {
            var aadhaarNumber = $('#AadhaarNumber').val();
            var otpValue = $('#Otp').val();
            if (otpValue == '' || otpValue.trim().length !== 6) {
                $('#Otp').focus();
                return false;
            }
            $.ajax({
                type: 'POST',
                url: "/frontend/web/aadhaarreverify/validateotp",
                data: {
                    AADHAARNUMBER: aadhaarNumber,
                    OTPVALUE: otpValue,
                    OTPTXNID: otpTxnId,
                },
                "beforeSend": function(response) {
                    SimpleLoading.start();
                },
                success: function(data) {
                    var resp = JSON.parse(data);
                    console.log(resp);
                    if (resp.error) {
                        $('#spnResult').html(resp.errorMessage);
                        $('#spnResult').css("color", "red");
                        $('#divMessage').show();
                        SimpleLoading.stop();
                        return false;
                    }
                },
                error: function(xhr, textStatus, error) {
                    $('#spnResult').html(error.message);
                    $('#spnResult').css("color", "red");
                    $('#divMessage').show();
                    SimpleLoading.stop();
                    return false;
                }
            });
        }
    </script>
